{
	"manifest_version": 2,
	"name": "QuickFront",
	"description": "Easily get all your workfront notifications.",
	"version": "1.2.8",
	"permissions": [
		"storage",
		"*://*/*",
		"notifications"
	],
	"background": {
		"scripts": ["js/jquery-2.0.3.min.js", "js/background.js"]
	},
	"icons": { 
		"16": "img/icon16.png",
		"48": "img/icon48.png",
		"128": "img/icon128.png" 
	},
	"browser_action": {
		"default_icon": {                    
			"19": "img/icon48.png",          
			"38": "img/icon48.png"          
		},
		"default_popup": "popup.html"
	},
	"content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
	"web_accessible_resources": [
		"img/icon128.png"
	]
}